let firstFavNumb = 07;
let secondFavNumb = 17;

console.log(firstFavNumb > secondFavNumb);
console.log(firstFavNumb < secondFavNumb);
console.log(firstFavNumb >= secondFavNumb);
console.log(firstFavNumb <= secondFavNumb);

console.log(firstFavNumb === secondFavNumb);
console.log(firstFavNumb !== secondFavNumb);
console.log(firstFavNumb == secondFavNumb);
console.log(firstFavNumb != secondFavNumb);
